{
  "name": "@prtty/prtty",
  "version": "1.0.0",
  "description": "A simple and minimalistic terminal styling library wrapper for Node.js default styleText",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "build": "tsdown --config-loader unrun",
    "prepack": "tsdown --config-loader unrun"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "engines": {
    "node": ">=20.12.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
